{
  "name": "email-templates",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "tools/email-templates",
  "projectType": "application",
  "tags": [],
  "// targets": "to see all targets run: nx show project email-templates --web",
  "targets": {
    "dev": {
      "command": "email dev --dir previews",
      "options": {
        "cwd": "tools/email-templates"
      }
    },
    "build": {
      "command": "email build --dir previews",
      "options": {
        "cwd": "tools/email-templates"
      }
    },
    "export": {
      "command": "email export --dir previews",
      "options": {
        "cwd": "tools/email-templates"
      }
    }
  }
}
