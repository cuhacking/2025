{
  "compilerOptions": {
    "target": "es2015",
    "lib": [
      "es2020",
      "dom"
    ],
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "baseUrl": ".",
    "rootDir": ".",
    "module": "esnext",
    "moduleResolution": "node",
    "paths": {
      "@payload-config": [
        "apps/axiom/src/payload.config.ts"
      ],
      "@aceternity": [
        "libs/external/aceternity/index.ts"
      ],
      "@aceternity/*": [
        "libs/external/aceternity/*"
      ],
      "@cuhacking/cms/*": [
        "libs/cms/*"
      ],
      "@/db": [
        "libs/db/index.ts"
      ],
      "@/db/*": [
        "libs/db/*"
      ],
      "@/engine": [
        "libs/engine/index.ts"
      ],
      "@/engine/*": [
        "libs/engine/*"
      ],
      "@cuhacking/docs": [
        "libs/docs/index.ts"
      ],
      "@cuhacking/docs/*": [
        "libs/docs/*"
      ],
      "@cuhacking/portal-e2e": [
        "libs/portal-e2e/index"
      ],
      "@cuhacking/portal-e2e/*": [
        "libs/portal-e2e/*"
      ],
      "@cuhacking/portal/*": [
        "libs/portal/*"
      ],
      "@cuhacking/shared": [
        "libs/shared/index.ts"
      ],
      "@cuhacking/shared/*": [
        "libs/shared/*"
      ],
      "@magic-ui": [
        "libs/external/magic-ui/index.ts"
      ],
      "@magic-ui/*": [
        "libs/external/magic-ui/*"
      ],
      "@shadcn": [
        "libs/external/shadcn/index.ts"
      ],
      "@shadcn/*": [
        "libs/external/shadcn/*"
      ],
      "@website-e2e/*": [
        "libs/website-e2e/*"
      ],
      "@website/*": [
        "libs/website/*"
      ]
    },
    "declaration": false,
    "importHelpers": true,
    "sourceMap": true,
    "skipDefaultLibCheck": true,
    "skipLibCheck": true
  },
  "include": [
    "eslint.config.js"
  ],
  "exclude": [
    "node_modules",
    "tmp"
  ],
  "compileOnSave": false
}
