{
  "name": "api",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "apps/api/src",
  "projectType": "application",
  "targets": {
    "dev": {
      "command": "cross-env NODE_OPTIONS=--no-deprecation next dev",
      "options": {
        "cwd": "apps/api"
      }
    },
    "devsafe": {
      "command": "rm -rf .next && cross-env NODE_OPTIONS=--no-deprecation next dev",
      "options": {
        "cwd": "apps/api"
      }
    },
    "build:cms": {
      "command": "cross-env NODE_OPTIONS=--no-deprecation next build",
      "options": {
        "cwd": "apps/api"
      }
    },
    "start": {
      "command": "cross-env NODE_OPTIONS=--no-deprecation next start",
      "options": {
        "cwd": "apps/api"
      }
    },
    "lint": {
      "command": "cross-env NODE_OPTIONS=--no-deprecation next lint",
      "options": {
        "cwd": "apps/api"
      }
    },
    "payload": {
      "command": "cross-env NODE_OPTIONS=--no-deprecation payload",
      "options": {
        "cwd": "apps/api"
      }
    },
    "importmap": {
      "command": "cross-env NODE_OPTIONS=--no-deprecation payload generate:importmap",
      "options": {
        "cwd": "apps/api"
      }
    },
    "types": {
      "command": "cross-env NODE_OPTIONS=--no-deprecation payload generate:types",
      "options": {
        "cwd": "apps/api"
      }
    },
    "build": {
      "executor": "nx:run-commands",
      "options": {
        "command": "webpack-cli build",
        "args": [
          "node-env=production"
        ]
      },
      "configurations": {
        "development": {
          "args": [
            "node-env=development"
          ]
        }
      }
    },
    "serve": {
      "executor": "@nx/js:node",
      "defaultConfiguration": "development",
      "dependsOn": [
        "build"
      ],
      "options": {
        "buildTarget": "nestjs:build",
        "runBuildTargetDependencies": false
      },
      "configurations": {
        "development": {
          "buildTarget": "nestjs:build:development"
        },
        "production": {
          "buildTarget": "nestjs:build:production"
        }
      }
    },
    "migrate:run": {
      "command": "cross-env NODE_OPTIONS=--no-deprecation payload migrate",
      "options": {
        "cwd": "apps/api"
      }
    },
    "migrate:create": {
      "command": "cross-env NODE_OPTIONS=--no-deprecation payload migrate:create",
      "options": {
        "cwd": "apps/api"
      }
    },
    "migrate:status": {
      "command": "cross-env NODE_OPTIONS=--no-deprecation payload migrate:status",
      "options": {
        "cwd": "apps/api"
      }
    },
    "migrate:down": {
      "command": "cross-env NODE_OPTIONS=--no-deprecation payload migrate:down",
      "options": {
        "cwd": "apps/api"
      }
    },
    "migrate:refresh": {
      "command": "cross-env NODE_OPTIONS=--no-deprecation payload migrate:refresh",
      "options": {
        "cwd": "apps/api"
      }
    },
    "migrate:reset": {
      "command": "cross-env NODE_OPTIONS=--no-deprecation payload migrate:reset",
      "options": {
        "cwd": "apps/api"
      }
    },
    "migrate:fresh": {
      "command": "cross-env NODE_OPTIONS=--no-deprecation payload migrate:fresh",
      "options": {
        "cwd": "apps/api"
      }
    }
  },
  "tags": []
}
